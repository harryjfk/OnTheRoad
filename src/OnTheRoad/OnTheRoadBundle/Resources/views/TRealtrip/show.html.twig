{% extends '::base.html.twig' %}

{% block body -%}
    <h1>TRealtrip</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Isclosed</th>
                <td>{{ entity.isclosed }}</td>
            </tr>
            <tr>
                <th>Dateinitrip</th>
                <td>{{ entity.dateinitrip|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Datefintrip</th>
                <td>{{ entity.datefintrip|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('trealtrip') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('trealtrip_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
